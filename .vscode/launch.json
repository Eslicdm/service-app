{
    "configurations": [
        {
            "type": "java",
            "name": "Spring Boot-ServiceAppRegistryApplication<service-app-registry>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.eslirodrigues.ServiceAppRegistryApplication",
            "projectName": "service-app-registry",
            "args": "",
            "envFile": "${workspaceFolder}/service-app-registry/.env",
            "vmArgs": " -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.port=53369 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Dspring.jmx.enabled=true -Djava.rmi.server.hostname=localhost -Dspring.application.admin.enabled=true -Dspring.boot.project.name=service-app-registry"
        },
